{
    "name": "gmponos/guzzle-logger",
    "description": "A middleware for guzzle to log every request and response",
    "license": "MIT",
    "homepage": "https://github.com/gmponos/Guzzle-logger",
    "authors": [
        {
            "name": "George Mponos",
            "email": "gmponos@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Gmponos\\GuzzleLogger\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gmponos\\GuzzleLogger\\Test\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.1",
        "guzzlehttp/guzzle": "^6.3",
        "psr/log": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "5.*",
        "webthink/codesniffer": "1.*"
    },
    "scripts": {
        "test": "php vendor/bin/phpunit",
        "phpcs": "php vendor/bin/phpcs ./src ./tests",
        "phpcbf": "php vendor/bin/phpcbf ./src ./tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "stable"
}
