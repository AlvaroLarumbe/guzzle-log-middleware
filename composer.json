{
    "name": "gmponos/guzzle_logger",
    "description": "A middleware for guzzle to log every request and response",
    "license": "MIT",
    "homepage": "https://github.com/gmponos/Guzzle-logger",
    "authors": [
        {
            "name": "George Mponos",
            "email": "gmponos@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Gmponos\\GuzzleLogger\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gmponos\\GuzzleLogger\\Test\\": "tests/"
        }
    },
    "require": {
        "php": ">=5.5",
        "guzzlehttp/guzzle": "^6.2",
        "psr/log": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "mockery/mockery": "*",
        "squizlabs/php_codesniffer": "2.*"
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "stable"
}
